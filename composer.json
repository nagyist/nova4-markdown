{
    "name": "nagyist/nova-markdown",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "erusev/parsedown": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Nagyist\\NovaMarkdown\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Nagyist\\NovaMarkdown\\MarkdownToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
